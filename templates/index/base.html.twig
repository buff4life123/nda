<!DOCTYPE html>
<html lang="{{local}}" xml:lang="{{local}}" xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-141915434-1"></script>
        <script>
          window.dataLayer = window.dataLayer || [];
          function gtag(){dataLayer.push(arguments);}
          gtag('js', new Date());
        
          gtag('config', 'UA-141915434-1');
        </script>

        <meta charset="UTF-8">
        <meta content="summary" name="twitter:card">
        <meta content="width=device-width, initial-scale=1" name="viewport">
        <meta content="Webflow" name="generator">
        <meta name="keywords" content="{{company.metaKeywords}}">
        <link rel="alternate" hreflang="{{local}}" href="https://www.nauticdrive-algarve.com">
        <meta name="description" content="{{company.metaDescription}}">
        
        <meta name="robots" content="index,nofollow">
 		
 		<!-- Google / Search Engine Tags -->
        <meta itemprop="name" content="NAUTICDRIVE - Desportos Náuticos na Praia da Rocha Baixinha, Albufeira">
        <meta itemprop="description" content="NAUTICDRIVE - Desportos Náuticos na Praia da Rocha Baixinha, Albufeira">
        <meta itemprop="image" content="https://nauticdrive-algarve.com/upload/gallery/f210acf07c12fc46a7457dbec347270c.png">
        
        <!-- Facebook Meta Tags -->
        <meta property="og:url" content="https://nauticdrive-algarve.com">
        <meta property="og:type" content="website">
        <meta property="og:title" content="NAUTICDRIVE - Desportos Náuticos na Praia da Rocha Baixinha, Albufeira">
        <meta property="og:description" content="NAUTICDRIVE - Desportos Náuticos na Praia da Rocha Baixinha, Albufeira">
        <meta property="og:image" content="https://nauticdrive-algarve.com/upload/gallery/f210acf07c12fc46a7457dbec347270c.png">
        
        <!-- Twitter Meta Tags -->
        <meta name="twitter:card" content="summary_large_image">
        <meta name="twitter:title" content="NAUTICDRIVE - Desportos Náuticos na Praia da Rocha Baixinha, Albufeira">
        <meta name="twitter:description" content="NAUTICDRIVE - Desportos Náuticos na Praia da Rocha Baixinha, Albufeira">
        <meta name="twitter:image" content="https://nauticdrive-algarve.com/upload/gallery/f210acf07c12fc46a7457dbec347270c.png">
        
        <title>{% block title %}{% trans %}company_name{%endtrans%}{% endblock %}</title>
        
        {% block stylesheets %}
            <!--Import slick.css-->
            <link type="text/css" rel="stylesheet" href="{{ asset('slick/slick.css') }}"  media="screen,projection"/>
            <link type="text/css" rel="stylesheet" href="{{ asset('slick/slick-theme.css') }}" />
           <!-- <link type="text/css" rel="stylesheet" href="{{ asset('slick/styles.css') }}" />-->

            <link href="{{ asset('css/w3.css') }}" rel="stylesheet" type="text/css">
            <link href="{{ asset('css/normalize.css') }}" rel="stylesheet" type="text/css">
            <link href="{{ asset('css/webflow.css') }}" rel="stylesheet" type="text/css">
            <link href="{{ asset('css/style.webflow.css') }}" rel="stylesheet" type="text/css">
            <link href="{{ asset('assets/css/font-awesome.min.css') }}" rel="stylesheet"/>
            <link href="{{ asset('assets/css/style.css') }}" rel="stylesheet"/>


        {% endblock %}

        {% set path = asset('/images/logo_nautic.ico')  %}
        <link rel="shortcut icon" type="image/x-icon" href="{{path}}"/>
        
        <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js" type="text/javascript"></script>
        <script type="text/javascript">
            WebFont.load({
                google: {
                    families: ["Varela:400", "Montserrat Subrayada:regular,700", "Barlow:200,200italic,300,300italic,regular,italic,500,600,700,800,900"]
                }
            });
        </script>


        <script crossorigin="anonymous" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" src="https://code.jquery.com/jquery-3.3.1.min.js" type="text/javascript"> </script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        <script type="text/javascript" src="{{ asset('slick/slick.min.js') }}"></script>
        <script src="{{ asset('js/instafeed.min.js') }}"></script>
        <script src="{{ asset('assets/js/parsley/highlight.min.js') }}"></script>
        <script src="{{ asset('assets/js/parsley/parsley.min.js') }}"></script>
    </head>
    
    {% include 'index/modals.html' %}
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        {% endblock %}
    </body>
</html>